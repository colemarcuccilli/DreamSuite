{
  "buildCommand": "expo export -p web",
  "outputDirectory": "dist",
  "devCommand": "expo start --web",
  "cleanUrls": true,
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "functions": {
    "app/api/payments/*.ts": {
      "maxDuration": 30
    },
    "app/api/email/*.ts": {
      "maxDuration": 10
    }
  }
}